npx create-next-app@latest toffee-ai --typescript --tailwind --eslint 
cd toffee-ai
npm install @types/react @types/react-dom @types/node
npm install next-auth @prisma/client prisma openai 
# Initialize the database
npx prisma db push
# Generate Prisma Client
npx prisma generate 
npm run dev 
node --version
